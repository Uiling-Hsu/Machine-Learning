package FileReader;

import java.io.BufferedReader;
import java.io.FileReader;
import java.util.ArrayList;


public class HwFileReader {

	
	public static void readFile(String name, ArrayList<double[]> source){
		String  thisLine = null;
		try{
	        // open input stream test.txt for reading purpose.
	        BufferedReader br = new BufferedReader(new FileReader(name));
	        
	        while ((thisLine = br.readLine()) != null) {
	           String sep[] = thisLine.split(" |\t");
	           double[] item = new double[6];
	           item[0]=1;
	           item[1]=Double.parseDouble(sep[0]);
	           item[2]=Double.parseDouble(sep[1]);
	           item[3]=Double.parseDouble(sep[2]);
	           item[4]=Double.parseDouble(sep[3]);
	           item[5]=Double.parseDouble(sep[4]);
	           source.add(item);
	        }       
	     }catch(Exception e){
	        e.printStackTrace();
	     }
	}
	
}
